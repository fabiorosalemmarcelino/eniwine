
@{
    ViewBag.Title = "CasoSolucionado";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-group">
    <h1><span class="label label-success">Você solucionou o caso !!! :)</span></h1>
</div>

<div class="form-group">
    <p>
        Deseja jogar novamente?
    </p>

    <input id="sim" type="button" value="Sim" class="btn btn-success" />
    <input id="nao" type="button" value="Não" class="btn btn-danger" />
</div>

<script src="~/Scripts/jquery-1.10.2.js"></script>
<script>

    $("#sim").click(function () {
        location.href = '/Jogo';
    });

    $("#nao").click(function () {
        location.href = '/Home';
    });
</script>




